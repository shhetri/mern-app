{
  "name": "mern-client",
  "version": "1.0.0",
  "scripts": {
    "dev": "next -- -p 8080",
    "build": "next build",
    "start": "next start",
    "pretty": "prettier --write \"./**/*.*\"",
    "lint:fix": "tslint -p . -c tslint.json -t stylish --fix"
  },
  "dependencies": {
    "@zeit/next-typescript": "1.1.1",
    "axios": "0.18.0",
    "next": "11.1.0",
    "react": "16.8.4",
    "react-dom": "16.8.4"
  },
  "devDependencies": {
    "@types/next": "8.0.1",
    "@types/react": "16.8.6",
    "@types/react-dom": "16.8.2",
    "husky": "1.3.1",
    "lint-staged": "8.1.4",
    "prettier": "1.16.4",
    "tslint": "5.13.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "3.6.0",
    "typescript": "3.3.3333"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "tslint --fix",
      "git add"
    ],
    "./**/*.*": [
      "prettier --write",
      "git add"
    ]
  },
  "license": "ISC"
}
